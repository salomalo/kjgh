<?xml version="1.0" encoding="utf-8"?>
<extension type="library" method="upgrade" version="2.5">
	<name>Techjoomla</name>
	<libraryname>techjoomla</libraryname>
	<author>Techjoomla</author>
	<authorEmail>extensions@techjoomla.com</authorEmail>
	<authorUrl>https://techjoomla.com</authorUrl>
	<copyright>Copyright (C) 2012 - 2019 Techjoomla. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<creationDate>22nd Oct 2019</creationDate>
	<version>1.2.2</version>
	<description>Common code library for all Techjoola extensions</description>
	<install>
		<sql>
			<file driver="mysql" charset="utf8">sql/install.sql</file>
		</sql>
	</install>
	<update>
		<schemas>
			<schemapath type="mysql">sql/updates/mysql</schemapath>
		</schemas>
	</update>
	<files>
		<folder>assets</folder>
		<folder>sql</folder>
		<folder>controller</folder>
		<folder>dompdf</folder>
		<folder>emogrifier</folder>
		<folder>form</folder>
		<folder>jsocial</folder>
		<folder>media</folder>
		<folder>model</folder>
		<folder>tjcsv</folder>
		<folder>tjmail</folder>
		<folder>tjmedia</folder>
		<folder>tjmoney</folder>
		<folder>tjtoolbar</folder>
		<folder>tjnotifications</folder>
		<folder>view</folder>
		<folder>tjfileviewer</folder>
		<file>common.php</file>
		<file>index.html</file>
		<file>techjoomla.xml</file>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.lib_techjoomla.ini</language>
		<language tag="en-GB">en-GB/en-GB.lib_techjoomla.sys.ini</language>
	</languages>
</extension>
